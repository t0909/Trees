int heapHeight(int N, int arr[])
    {
        int count = 0;
        
        for(int i=0; i<N; i= 2*i+1)
        {
           if(arr[i]!= NULL) 
           {
            count++;
               
           }
           
           else 
           return count-1;
            
        }
        
        return count-1;
        
    }
